    {
      "sortKey": 5000,
      "text": "مِن قَبۡلُ وَرُسُلࣰا لَّمۡ نَقۡصُصۡهُمۡ عَلَیۡكَۚ وَكَلَّمَ ٱللَّهُ مُوسَىٰ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 164,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "تَكۡلِیمࣰا ۝١٦٤ رُّسُلࣰا مُّبَشِّرِینَ وَمُنذِرِینَ لِئَلَّا یَكُونَ لِلنَّاسِ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 164,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "عَلَى ٱللَّهِ حُجَّةُۢ بَعۡدَ ٱلرُّسُلِۚ وَكَانَ ٱللَّهُ عَزِیزًا حَكِیمࣰا ۝١٦٥",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 165,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "لَّـٰكِنِ ٱللَّهُ یَشۡهَدُ بِمَاۤ أَنزَلَ إِلَیۡكَۖ أَنزَلَهُۥ بِعِلۡمِهِۦۖ وَٱلۡمَلَـٰۤىِٕكَةُ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 166,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "یَشۡهَدُونَۚ وَكَفَىٰ بِٱللَّهِ شَهِیدًا ۝١٦٦ إِنَّ ٱلَّذِینَ كَفَرُوا۟",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 166,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "وَصَدُّوا۟ عَن سَبِیلِ ٱللَّهِ قَدۡ ضَلُّوا۟ ضَلَـٰلَۢا بَعِیدًا ۝١٦٧ إِنَّ ٱلَّذِینَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 167,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 11000,
      "text": "كَفَرُوا۟ وَظَلَمُوا۟ لَمۡ یَكُنِ ٱللَّهُ لِیَغۡفِرَ لَهُمۡ وَلَا لِیَهۡدِیَهُمۡ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 168,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 12000,
      "text": "طَرِیقًا ۝١٦٨ إِلَّا طَرِیقَ جَهَنَّمَ خَـٰلِدِینَ فِیهَاۤ أَبَدࣰاۚ وَكَانَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 168,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 13000,
      "text": "ذَ ٰ⁠لِكَ عَلَى ٱللَّهِ یَسِیرࣰا ۝١٦٩ یَـٰۤأَیُّهَا ٱلنَّاسُ قَدۡ جَاۤءَكُمُ ٱلرَّسُولُ بِٱلۡحَقِّ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 169,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 14000,
      "text": "مِن رَّبِّكُمۡ فَـَٔامِنُوا۟ خَیۡرࣰا لَّكُمۡۚ وَإِن تَكۡفُرُوا۟ فَإِنَّ لِلَّهِ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 170,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 15000,
      "text": "مَا فِی ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضِۚ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا ۝١٧٠",
      "surahIndexFirst": 4,
