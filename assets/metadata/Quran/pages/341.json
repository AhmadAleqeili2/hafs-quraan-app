      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "إِنَّ ٱللَّهَ لَقَوِیٌّ عَزِیزٌ ۝٧٤ ٱللَّهُ یَصۡطَفِی مِنَ ٱلۡمَلَـٰۤىِٕكَةِ",
      "surahIndexFirst": 22,
      "ayahIndexFirst": 74,
      "isBasmala": false,
      "isTitle": false,
      "semantic": [
        {
          "tokens": "مَا قَدَرُوا۟ ٱللَّهَ",
