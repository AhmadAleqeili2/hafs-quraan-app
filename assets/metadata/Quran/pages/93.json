    {
      "sortKey": 1000,
      "text": "وَمَا كَانَ لِمُؤۡمِنٍ أَن یَقۡتُلَ مُؤۡمِنًا إِلَّا خَطَـࣰٔاۚ وَمَن قَتَلَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "مُؤۡمِنًا خَطَـࣰٔا فَتَحۡرِیرُ رَقَبَةࣲ مُّؤۡمِنَةࣲ وَدِیَةࣱ مُّسَلَّمَةٌ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "إِلَىٰۤ أَهۡلِهِۦۤ إِلَّاۤ أَن یَصَّدَّقُوا۟ۚ فَإِن كَانَ مِن قَوۡمٍ عَدُوࣲّ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "لَّكُمۡ وَهُوَ مُؤۡمِنࣱ فَتَحۡرِیرُ رَقَبَةࣲ مُّؤۡمِنَةࣲۖ وَإِن كَانَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "مِن قَوۡمِۭ بَیۡنَكُمۡ وَبَیۡنَهُم مِّیثَـٰقࣱ فَدِیَةࣱ مُّسَلَّمَةٌ إِلَىٰۤ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "أَهۡلِهِۦ وَتَحۡرِیرُ رَقَبَةࣲ مُّؤۡمِنَةࣲۖ فَمَن لَّمۡ یَجِدۡ فَصِیَامُ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "شَهۡرَیۡنِ مُتَتَابِعَیۡنِ تَوۡبَةࣰ مِّنَ ٱللَّهِۗ وَكَانَ ٱللَّهُ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "عَلِیمًا حَكِیمࣰا ۝٩٢ وَمَن یَقۡتُلۡ مُؤۡمِنࣰا مُّتَعَمِّدࣰا",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "فَجَزَاۤؤُهُۥ جَهَنَّمُ خَـٰلِدࣰا فِیهَا وَغَضِبَ ٱللَّهُ عَلَیۡهِ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 93,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "وَلَعَنَهُۥ وَأَعَدَّ لَهُۥ عَذَابًا عَظِیمࣰا ۝٩٣ یَـٰۤأَیُّهَا ٱلَّذِینَ",
