    {
      "sortKey": 1000,
      "text": "إِنَّمَا یُرِیدُ ٱلشَّیۡطَـٰنُ أَن یُوقِعَ بَیۡنَكُمُ ٱلۡعَدَ ٰ⁠وَةَ وَٱلۡبَغۡضَاۤءَ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 91,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "فِی ٱلۡخَمۡرِ وَٱلۡمَیۡسِرِ وَیَصُدَّكُمۡ عَن ذِكۡرِ ٱللَّهِ وَعَنِ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 91,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "ٱلصَّلَوٰةِۖ فَهَلۡ أَنتُم مُّنتَهُونَ ۝٩١ وَأَطِیعُوا۟ ٱللَّهَ وَأَطِیعُوا۟",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 91,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ٱلرَّسُولَ وَٱحۡذَرُوا۟ۚ فَإِن تَوَلَّیۡتُمۡ فَٱعۡلَمُوۤا۟ أَنَّمَا عَلَىٰ رَسُولِنَا",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 92,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "ٱلۡبَلَـٰغُ ٱلۡمُبِینُ ۝٩٢ لَیۡسَ عَلَى ٱلَّذِینَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ",
