    {
      "sortKey": 1000,
      "text": "وَإِن كُنتُمۡ عَلَىٰ سَفَرࣲ وَلَمۡ تَجِدُوا۟ كَاتِبࣰا فَرِهَـٰنࣱ مَّقۡبُوضَةࣱۖ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 283,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "فَإِنۡ أَمِنَ بَعۡضُكُم بَعۡضࣰا فَلۡیُؤَدِّ ٱلَّذِی ٱؤۡتُمِنَ أَمَـٰنَتَهُۥ وَلۡیَتَّقِ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 283,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "ٱللَّهَ رَبَّهُۥۗ وَلَا تَكۡتُمُوا۟ ٱلشَّهَـٰدَةَۚ وَمَن یَكۡتُمۡهَا فَإِنَّهُۥۤ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 283,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ءَاثِمࣱ قَلۡبُهُۥۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ عَلِیمࣱ ۝٢٨٣ لِّلَّهِ مَا فِی ٱلسَّمَـٰوَ ٰ⁠تِ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 283,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "وَمَا فِی ٱلۡأَرۡضِۗ وَإِن تُبۡدُوا۟ مَا فِیۤ أَنفُسِكُمۡ أَوۡ تُخۡفُوهُ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 284,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "یُحَاسِبۡكُم بِهِ ٱللَّهُۖ فَیَغۡفِرُ لِمَن یَشَاۤءُ وَیُعَذِّبُ مَن یَشَاۤءُۗ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 284,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءࣲ قَدِیرٌ ۝٢٨٤ ءَامَنَ ٱلرَّسُولُ بِمَاۤ أُنزِلَ إِلَیۡهِ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 284,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "مِن رَّبِّهِۦ وَٱلۡمُؤۡمِنُونَۚ كُلٌّ ءَامَنَ بِٱللَّهِ وَمَلَـٰۤىِٕكَتِهِۦ",
      "surahIndexFirst": 2,
