      "ayahIndexFirst": 148,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "جَمِیعًاۚ إِنَّ ٱللَّهَ عَلَىٰ كُلِّ شَیۡءࣲ قَدِیرࣱ ۝١٤٨ وَمِنۡ حَیۡثُ خَرَجۡتَ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 148,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "فَوَلِّ وَجۡهَكَ شَطۡرَ ٱلۡمَسۡجِدِ ٱلۡحَرَامِۖ وَإِنَّهُۥ لَلۡحَقُّ مِن رَّبِّكَۗ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 149,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "وَمَا ٱللَّهُ بِغَـٰفِلٍ عَمَّا تَعۡمَلُونَ ۝١٤٩ وَمِنۡ حَیۡثُ خَرَجۡتَ فَوَلِّ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 149,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "وَجۡهَكَ شَطۡرَ ٱلۡمَسۡجِدِ ٱلۡحَرَامِۚ وَحَیۡثُ مَا كُنتُمۡ فَوَلُّوا۟",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "وُجُوهَكُمۡ شَطۡرَهُۥ لِئَلَّا یَكُونَ لِلنَّاسِ عَلَیۡكُمۡ حُجَّةٌ إِلَّا ٱلَّذِینَ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "ظَلَمُوا۟ مِنۡهُمۡ فَلَا تَخۡشَوۡهُمۡ وَٱخۡشَوۡنِی وَلِأُتِمَّ نِعۡمَتِی عَلَیۡكُمۡ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 11000,
      "text": "وَلَعَلَّكُمۡ تَهۡتَدُونَ ۝١٥٠ كَمَاۤ أَرۡسَلۡنَا فِیكُمۡ رَسُولࣰا مِّنكُمۡ یَتۡلُوا۟",
