    },
    {
      "sortKey": 5000,
      "text": "ءَامَنُوۤا۟ إِنَّ كَثِیرࣰا مِّنَ ٱلۡأَحۡبَارِ وَٱلرُّهۡبَانِ لَیَأۡكُلُونَ",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 34,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "أَمۡوَ ٰ⁠لَ ٱلنَّاسِ بِٱلۡبَـٰطِلِ وَیَصُدُّونَ عَن سَبِیلِ ٱللَّهِۗ وَٱلَّذِینَ",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 34,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "یَكۡنِزُونَ ٱلذَّهَبَ وَٱلۡفِضَّةَ وَلَا یُنفِقُونَهَا فِی",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 34,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "سَبِیلِ ٱللَّهِ فَبَشِّرۡهُم بِعَذَابٍ أَلِیمࣲ ۝٣٤ یَوۡمَ یُحۡمَىٰ عَلَیۡهَا",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 34,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "فِی نَارِ جَهَنَّمَ فَتُكۡوَىٰ بِهَا جِبَاهُهُمۡ وَجُنُوبُهُمۡ",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 35,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "وَظُهُورُهُمۡۖ هَـٰذَا مَا كَنَزۡتُمۡ لِأَنفُسِكُمۡ فَذُوقُوا۟ مَا كُنتُمۡ",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 35,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 11000,
      "text": "تَكۡنِزُونَ ۝٣٥ إِنَّ عِدَّةَ ٱلشُّهُورِ عِندَ ٱللَّهِ ٱثۡنَا عَشَرَ",
      "surahIndexFirst": 9,
      "ayahIndexFirst": 35,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 12000,
      "text": "شَهۡرࣰا فِی كِتَـٰبِ ٱللَّهِ یَوۡمَ خَلَقَ ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضَ مِنۡهَاۤ",
