      "sortKey": 1000,
      "text": "یُقَلِّبُ ٱللَّهُ ٱلَّیۡلَ وَٱلنَّهَارَۚ إِنَّ فِی ذَ ٰ⁠لِكَ لَعِبۡرَةࣰ لِّأُو۟لِی ٱلۡأَبۡصَـٰرِ ۝٤٤",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 44,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "وَٱللَّهُ خَلَقَ كُلَّ دَاۤبَّةࣲ مِّن مَّاۤءࣲۖ فَمِنۡهُم مَّن یَمۡشِی عَلَىٰ بَطۡنِهِۦ وَمِنۡهُم مَّن",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 45,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "یَمۡشِی عَلَىٰ رِجۡلَیۡنِ وَمِنۡهُم مَّن یَمۡشِی عَلَىٰۤ أَرۡبَعࣲۚ یَخۡلُقُ ٱللَّهُ مَا یَشَاۤءُۚ",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 45,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "إِنَّ ٱللَّهَ عَلَىٰ كُلِّ شَیۡءࣲ قَدِیرࣱ ۝٤٥ لَّقَدۡ أَنزَلۡنَاۤ ءَایَـٰتࣲ مُّبَیِّنَـٰتࣲۚ",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 45,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "وَٱللَّهُ یَهۡدِی مَن یَشَاۤءُ إِلَىٰ صِرَ ٰ⁠طࣲ مُّسۡتَقِیمࣲ ۝٤٦ وَیَقُولُونَ",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 46,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "ءَامَنَّا بِٱللَّهِ وَبِٱلرَّسُولِ وَأَطَعۡنَا ثُمَّ یَتَوَلَّىٰ فَرِیقࣱ مِّنۡهُم مِّنۢ بَعۡدِ",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 47,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "ذَ ٰ⁠لِكَۚ وَمَاۤ أُو۟لَـٰۤىِٕكَ بِٱلۡمُؤۡمِنِینَ ۝٤٧ وَإِذَا دُعُوۤا۟ إِلَى ٱللَّهِ وَرَسُولِهِۦ",
      "surahIndexFirst": 24,
      "ayahIndexFirst": 47,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "لِیَحۡكُمَ بَیۡنَهُمۡ إِذَا فَرِیقࣱ مِّنۡهُم مُّعۡرِضُونَ ۝٤٨ وَإِن یَكُن لَّهُمُ ٱلۡحَقُّ",
