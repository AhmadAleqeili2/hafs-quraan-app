    {
      "sortKey": 1000,
      "text": "وَإِذۡ قَالَ عِیسَى ٱبۡنُ مَرۡیَمَ یَـٰبَنِیۤ إِسۡرَ ٰ⁠ۤءِیلَ إِنِّی رَسُولُ ٱللَّهِ إِلَیۡكُم مُّصَدِّقࣰا لِّمَا",
      "surahIndexFirst": 61,
      "ayahIndexFirst": 6,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "بَیۡنَ یَدَیَّ مِنَ ٱلتَّوۡرَىٰةِ وَمُبَشِّرَۢا بِرَسُولࣲ یَأۡتِی مِنۢ بَعۡدِی ٱسۡمُهُۥۤ أَحۡمَدُۖ فَلَمَّا",
      "surahIndexFirst": 61,
      "ayahIndexFirst": 6,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "جَاۤءَهُم بِٱلۡبَیِّنَـٰتِ قَالُوا۟ هَـٰذَا سِحۡرࣱ مُّبِینࣱ ۝٦ وَمَنۡ أَظۡلَمُ مِمَّنِ ٱفۡتَرَىٰ عَلَى",
      "surahIndexFirst": 61,
      "ayahIndexFirst": 6,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ٱللَّهِ ٱلۡكَذِبَ وَهُوَ یُدۡعَىٰۤ إِلَى ٱلۡإِسۡلَـٰمِۚ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلظَّـٰلِمِینَ ۝٧",
      "surahIndexFirst": 61,
      "ayahIndexFirst": 7,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "یُرِیدُونَ لِیُطۡفِـُٔوا۟ نُورَ ٱللَّهِ بِأَفۡوَ ٰ⁠هِهِمۡ وَٱللَّهُ مُتِمُّ نُورِهِۦ وَلَوۡ كَرِهَ",
      "surahIndexFirst": 61,
      "ayahIndexFirst": 8,
      "isBasmala": false,
      "isTitle": false,
      "semantic": [
        {
