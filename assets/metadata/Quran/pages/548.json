    {
      "sortKey": 1000,
      "text": "فَكَانَ عَـٰقِبَتَهُمَاۤ أَنَّهُمَا فِی ٱلنَّارِ خَـٰلِدَیۡنِ فِیهَاۚ",
      "surahIndexFirst": 59,
      "ayahIndexFirst": 17,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "وَذَ ٰ⁠لِكَ جَزَ ٰ⁠ۤؤُا۟ ٱلظَّـٰلِمِینَ ۝١٧ یَـٰۤأَیُّهَا ٱلَّذِینَ ءَامَنُوا۟",
      "surahIndexFirst": 59,
      "ayahIndexFirst": 17,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "ٱتَّقُوا۟ ٱللَّهَ وَلۡتَنظُرۡ نَفۡسࣱ مَّا قَدَّمَتۡ لِغَدࣲۖ وَٱتَّقُوا۟ ٱللَّهَۚ إِنَّ",
      "surahIndexFirst": 59,
      "ayahIndexFirst": 18,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ٱللَّهَ خَبِیرُۢ بِمَا تَعۡمَلُونَ ۝١٨ وَلَا تَكُونُوا۟ كَٱلَّذِینَ",
      "surahIndexFirst": 59,
      "ayahIndexFirst": 18,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "نَسُوا۟ ٱللَّهَ فَأَنسَىٰهُمۡ أَنفُسَهُمۡۚ أُو۟لَـٰۤىِٕكَ هُمُ ٱلۡفَـٰسِقُونَ ۝١٩",
      "surahIndexFirst": 59,
      "ayahIndexFirst": 19,
      "isBasmala": false,
      "isTitle": false,
      "semantic": [
