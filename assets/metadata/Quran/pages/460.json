      "sortKey": 1000,
      "text": "قُلۡ إِنِّیۤ أُمِرۡتُ أَنۡ أَعۡبُدَ ٱللَّهَ مُخۡلِصࣰا لَّهُ ٱلدِّینَ ۝١١ وَأُمِرۡتُ لِأَنۡ أَكُونَ",
      "surahIndexFirst": 39,
      "ayahIndexFirst": 11,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "أَوَّلَ ٱلۡمُسۡلِمِینَ ۝١٢ قُلۡ إِنِّیۤ أَخَافُ إِنۡ عَصَیۡتُ رَبِّی عَذَابَ یَوۡمٍ",
      "surahIndexFirst": 39,
      "ayahIndexFirst": 12,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "عَظِیمࣲ ۝١٣ قُلِ ٱللَّهَ أَعۡبُدُ مُخۡلِصࣰا لَّهُۥ دِینِی ۝١٤ فَٱعۡبُدُوا۟ مَا شِئۡتُم مِّن",
      "surahIndexFirst": 39,
      "ayahIndexFirst": 13,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "دُونِهِۦۗ قُلۡ إِنَّ ٱلۡخَـٰسِرِینَ ٱلَّذِینَ خَسِرُوۤا۟ أَنفُسَهُمۡ وَأَهۡلِیهِمۡ یَوۡمَ ٱلۡقِیَـٰمَةِۗ",
      "surahIndexFirst": 39,
      "ayahIndexFirst": 15,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "أَلَا ذَ ٰ⁠لِكَ هُوَ ٱلۡخُسۡرَانُ ٱلۡمُبِینُ ۝١٥ لَهُم مِّن فَوۡقِهِمۡ ظُلَلࣱ مِّنَ ٱلنَّارِ",
      "surahIndexFirst": 39,
      "ayahIndexFirst": 15,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "وَمِن تَحۡتِهِمۡ ظُلَلࣱۚ ذَ ٰ⁠لِكَ یُخَوِّفُ ٱللَّهُ بِهِۦ عِبَادَهُۥۚ یَـٰعِبَادِ فَٱتَّقُونِ ۝١٦",
      "surahIndexFirst": 39,
