    {
      "sortKey": 1000,
      "text": "لَّا یُحِبُّ ٱللَّهُ ٱلۡجَهۡرَ بِٱلسُّوۤءِ مِنَ ٱلۡقَوۡلِ إِلَّا مَن ظُلِمَۚ وَكَانَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 148,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "ٱللَّهُ سَمِیعًا عَلِیمًا ۝١٤٨ إِن تُبۡدُوا۟ خَیۡرًا أَوۡ تُخۡفُوهُ أَوۡ تَعۡفُوا۟ عَن",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 148,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "سُوۤءࣲ فَإِنَّ ٱللَّهَ كَانَ عَفُوࣰّا قَدِیرًا ۝١٤٩ إِنَّ ٱلَّذِینَ یَكۡفُرُونَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 149,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "بِٱللَّهِ وَرُسُلِهِۦ وَیُرِیدُونَ أَن یُفَرِّقُوا۟ بَیۡنَ ٱللَّهِ وَرُسُلِهِۦ وَیَقُولُونَ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "نُؤۡمِنُ بِبَعۡضࣲ وَنَكۡفُرُ بِبَعۡضࣲ وَیُرِیدُونَ أَن یَتَّخِذُوا۟",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "بَیۡنَ ذَ ٰ⁠لِكَ سَبِیلًا ۝١٥٠ أُو۟لَـٰۤىِٕكَ هُمُ ٱلۡكَـٰفِرُونَ حَقࣰّاۚ وَأَعۡتَدۡنَا",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 150,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "لِلۡكَـٰفِرِینَ عَذَابࣰا مُّهِینࣰا ۝١٥١ وَٱلَّذِینَ ءَامَنُوا۟ بِٱللَّهِ وَرُسُلِهِۦ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 151,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "وَلَمۡ یُفَرِّقُوا۟ بَیۡنَ أَحَدࣲ مِّنۡهُمۡ أُو۟لَـٰۤىِٕكَ سَوۡفَ یُؤۡتِیهِمۡ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 152,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "أُجُورَهُمۡۚ وَكَانَ ٱللَّهُ غَفُورࣰا رَّحِیمࣰا ۝١٥٢ یَسۡـَٔلُكَ أَهۡلُ ٱلۡكِتَـٰبِ",
      "surahIndexFirst": 4,
      "ayahIndexFirst": 152,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "أَن تُنَزِّلَ عَلَیۡهِمۡ كِتَـٰبࣰا مِّنَ ٱلسَّمَاۤءِۚ فَقَدۡ سَأَلُوا۟ مُوسَىٰۤ أَكۡبَرَ",
