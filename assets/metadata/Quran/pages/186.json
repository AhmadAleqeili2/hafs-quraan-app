    },
    {
      "sortKey": 5000,
      "text": "ءَامَنُوا۟ وَهَاجَرُوا۟ وَجَـٰهَدُوا۟ بِأَمۡوَ ٰ⁠لِهِمۡ وَأَنفُسِهِمۡ فِی سَبِیلِ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 72,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "ٱللَّهِ وَٱلَّذِینَ ءَاوَوا۟ وَّنَصَرُوۤا۟ أُو۟لَـٰۤىِٕكَ بَعۡضُهُمۡ أَوۡلِیَاۤءُ بَعۡضࣲۚ وَٱلَّذِینَ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 72,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "ءَامَنُوا۟ وَلَمۡ یُهَاجِرُوا۟ مَا لَكُم مِّن وَلَـٰیَتِهِم مِّن شَیۡءٍ حَتَّىٰ یُهَاجِرُوا۟ۚ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 72,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "وَإِنِ ٱسۡتَنصَرُوكُمۡ فِی ٱلدِّینِ فَعَلَیۡكُمُ ٱلنَّصۡرُ إِلَّا عَلَىٰ قَوۡمِۭ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 72,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "بَیۡنَكُمۡ وَبَیۡنَهُم مِّیثَـٰقࣱۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ بَصِیرࣱ ۝٧٢ وَٱلَّذِینَ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 72,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 10000,
      "text": "كَفَرُوا۟ بَعۡضُهُمۡ أَوۡلِیَاۤءُ بَعۡضٍۚ إِلَّا تَفۡعَلُوهُ تَكُن فِتۡنَةࣱ فِی",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 73,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 11000,
      "text": "ٱلۡأَرۡضِ وَفَسَادࣱ كَبِیرࣱ ۝٧٣ وَٱلَّذِینَ ءَامَنُوا۟ وَهَاجَرُوا۟ وَجَـٰهَدُوا۟",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 73,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 12000,
      "text": "فِی سَبِیلِ ٱللَّهِ وَٱلَّذِینَ ءَاوَوا۟ وَّنَصَرُوۤا۟ أُو۟لَـٰۤىِٕكَ هُمُ ٱلۡمُؤۡمِنُونَ",
      "surahIndexFirst": 8,
      "ayahIndexFirst": 74,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 13000,
