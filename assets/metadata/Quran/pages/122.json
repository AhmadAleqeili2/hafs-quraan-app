      "ayahIndexFirst": 84,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "رَبُّنَا مَعَ ٱلۡقَوۡمِ ٱلصَّـٰلِحِینَ ۝٨٤ فَأَثَـٰبَهُمُ ٱللَّهُ بِمَا قَالُوا۟ جَنَّـٰتࣲ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 84,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "تَجۡرِی مِن تَحۡتِهَا ٱلۡأَنۡهَـٰرُ خَـٰلِدِینَ فِیهَاۚ وَذَ ٰ⁠لِكَ جَزَاۤءُ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 85,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ٱلۡمُحۡسِنِینَ ۝٨٥ وَٱلَّذِینَ كَفَرُوا۟ وَكَذَّبُوا۟ بِـَٔایَـٰتِنَاۤ أُو۟لَـٰۤىِٕكَ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 85,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "أَصۡحَـٰبُ ٱلۡجَحِیمِ ۝٨٦ یَـٰۤأَیُّهَا ٱلَّذِینَ ءَامَنُوا۟ لَا تُحَرِّمُوا۟",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 86,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "طَیِّبَـٰتِ مَاۤ أَحَلَّ ٱللَّهُ لَكُمۡ وَلَا تَعۡتَدُوۤا۟ۚ إِنَّ ٱللَّهَ لَا یُحِبُّ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 87,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "ٱلۡمُعۡتَدِینَ ۝٨٧ وَكُلُوا۟ مِمَّا رَزَقَكُمُ ٱللَّهُ حَلَـٰلࣰا طَیِّبࣰاۚ",
      "surahIndexFirst": 5,
      "ayahIndexFirst": 87,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "وَٱتَّقُوا۟ ٱللَّهَ ٱلَّذِیۤ أَنتُم بِهِۦ مُؤۡمِنُونَ ۝٨٨ لَا یُؤَاخِذُكُمُ ٱللَّهُ",
