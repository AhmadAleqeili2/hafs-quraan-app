      "sortKey": 1000,
      "text": "قَالُوا۟ یَـٰمُوسَىٰۤ إِمَّاۤ أَن تُلۡقِیَ وَإِمَّاۤ أَن نَّكُونَ أَوَّلَ مَنۡ أَلۡقَىٰ ۝٦٥ قَالَ بَلۡ",
      "surahIndexFirst": 20,
      "ayahIndexFirst": 65,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "أَلۡقُوا۟ۖ فَإِذَا حِبَالُهُمۡ وَعِصِیُّهُمۡ یُخَیَّلُ إِلَیۡهِ مِن سِحۡرِهِمۡ أَنَّهَا",
      "surahIndexFirst": 20,
      "ayahIndexFirst": 66,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "تَسۡعَىٰ ۝٦٦ فَأَوۡجَسَ فِی نَفۡسِهِۦ خِیفَةࣰ مُّوسَىٰ ۝٦٧ قُلۡنَا لَا تَخَفۡ إِنَّكَ",
      "surahIndexFirst": 20,
      "ayahIndexFirst": 66,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "أَنتَ ٱلۡأَعۡلَىٰ ۝٦٨ وَأَلۡقِ مَا فِی یَمِینِكَ تَلۡقَفۡ مَا صَنَعُوۤا۟ۖ إِنَّمَا صَنَعُوا۟",
      "surahIndexFirst": 20,
      "ayahIndexFirst": 68,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "كَیۡدُ سَـٰحِرࣲۖ وَلَا یُفۡلِحُ ٱلسَّاحِرُ حَیۡثُ أَتَىٰ ۝٦٩ فَأُلۡقِیَ ٱلسَّحَرَةُ سُجَّدࣰا",
      "surahIndexFirst": 20,
      "ayahIndexFirst": 69,
      "isBasmala": false,
      "isTitle": false,
      "semantic": [
        {
          "tokens": "تَلْقَفْ",
