    {
      "sortKey": 1000,
      "text": "وَمَاۤ أَنفَقۡتُم مِّن نَّفَقَةٍ أَوۡ نَذَرۡتُم مِّن نَّذۡرࣲ فَإِنَّ ٱللَّهَ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 270,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 2000,
      "text": "یَعۡلَمُهُۥۗ وَمَا لِلظَّـٰلِمِینَ مِنۡ أَنصَارٍ ۝٢٧٠ إِن تُبۡدُوا۟",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 270,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 3000,
      "text": "ٱلصَّدَقَـٰتِ فَنِعِمَّا هِیَۖ وَإِن تُخۡفُوهَا وَتُؤۡتُوهَا",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 271,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 4000,
      "text": "ٱلۡفُقَرَاۤءَ فَهُوَ خَیۡرࣱ لَّكُمۡۚ وَیُكَفِّرُ عَنكُم مِّن",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 271,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 5000,
      "text": "سَیِّـَٔاتِكُمۡۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِیرࣱ ۝٢٧١ لَّیۡسَ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 271,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 6000,
      "text": "عَلَیۡكَ هُدَىٰهُمۡ وَلَـٰكِنَّ ٱللَّهَ یَهۡدِی مَن یَشَاۤءُۗ وَمَا",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 272,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 7000,
      "text": "تُنفِقُوا۟ مِنۡ خَیۡرࣲ فَلِأَنفُسِكُمۡۚ وَمَا تُنفِقُونَ إِلَّا",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 272,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 8000,
      "text": "ٱبۡتِغَاۤءَ وَجۡهِ ٱللَّهِۚ وَمَا تُنفِقُوا۟ مِنۡ خَیۡرࣲ یُوَفَّ إِلَیۡكُمۡ",
      "surahIndexFirst": 2,
      "ayahIndexFirst": 272,
      "isBasmala": false,
      "isTitle": false
    },
    {
      "sortKey": 9000,
      "text": "وَأَنتُمۡ لَا تُظۡلَمُونَ ۝٢٧٢ لِلۡفُقَرَاۤءِ ٱلَّذِینَ أُحۡصِرُوا۟",
